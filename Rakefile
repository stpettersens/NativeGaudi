# 
# Rakefile to build and test NativeGaudi on Travis CI.
# To build on your own system; please use `make` instead.
#

task :default => [:deps, :build, :test, :clean]

task :deps do
	puts "TODO: Get dependencies..."
end

task :build do
	sh "make"
end

task :test do
	sh "make test"
end

task :clean do
	sh "make clean"
end
